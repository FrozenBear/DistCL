<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="basicHttpEndpoint_Compiler" maxReceivedMessageSize="2147483647" />
                <binding name="basicHttpEndpoint_CompileManager" />
                <binding name="basicHttpEndpoint_LocalCompiler" />
                <binding name="basicHttpEndpoint_AgentPool" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="netTcpEndpoint" transferMode="Streamed" maxReceivedMessageSize="2147483647" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="http://npopov:8733/Compile/Local" binding="basicHttpBinding"
                bindingConfiguration="basicHttpEndpoint_LocalCompiler" contract="CompileService.ILocalCompiler"
                name="basicHttpEndpoint_LocalCompiler" />
            <endpoint address="http://localhost:8733/Compile/Compiler" binding="basicHttpBinding"
                bindingConfiguration="basicHttpEndpoint_Compiler" contract="CompileService.ICompiler"
                name="basicHttpEndpoint_Compiler" />
            <endpoint address="http://localhost:8733/Compile/AgentPool" binding="basicHttpBinding"
                bindingConfiguration="basicHttpEndpoint_AgentPool" contract="CompileService.IAgentPool"
                name="basicHttpEndpoint_AgentPool" />
            <endpoint address="http://localhost:8733/Compile/Manager" binding="basicHttpBinding"
                bindingConfiguration="basicHttpEndpoint_CompileManager" contract="CompileService.ICompileManager"
                name="basicHttpEndpoint_CompileManager" />
            <endpoint address="net.tcp://localhost:8734/Compile" binding="netTcpBinding"
                bindingConfiguration="netTcpEndpoint" contract="CompileService.ICompiler"
                name="netTcpEndpoint">
                <identity>
                    <userPrincipalName value="npopov\profiler" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>